 library(ggplot2)
 library(dplyr)
 library(psych)
library(MatchIt)
library(vcd)
library("cobalt")
library(DMwR2)
um <- read.csv(file = "PSM12.csv", header = TRUE, sep = ";")
colmissing <- apply(um, 2, function(x){sum(is.na(x))})
colmissing
um.c <- knnImputation(um, k = 10, meth = 'weighAvg')
colmissing <- apply(um.c, 2, function(x){sum(is.na(x))})
colmissing
mod_match <- matchit(Plasma ~ x.sex + x.age + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.2, data = um.c)
mod_match
summary(mod_match, un =TRUE)
love.plot(mod_match, binary = "std")
mod_match <- matchit(Plasma ~ x.sex + x.age+ x.BMI + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.2, data = um.c)
mod_match
summary(mod_match, un =TRUE)
mod_match <- matchit(Plasma ~ x.age+ x.BMI + x.CT + x.CRP + x.SpO2, method = 'nearest',exact = 'x.sex', distance = 'glm', caliper = 0.2, data = um.c)
summary(mod_match, un =TRUE)
love.plot(mod_match, binary = "std")
mod_match <- matchit(Plasma ~ x.age+ x.BMI + x.CT + x.CRP + x.SpO2, method = 'nearest',exact = 'x.sex', distance = 'glm', caliper = 0.4, data = um.c)
love.plot(mod_match, binary = "std")
summary(mod_match, un =TRUE)
bal.plot(mod_match, var.name = "distance", which = "both", type = "histogram", mirror = TRUE)
mod_match <- matchit(Plasma ~ x.age+ x.BMI + x.CT + x.CRP + x.SpO2, method = 'nearest',exact = 'x.sex', distance = 'glm', caliper = 0.5, data = um.c)
bal.plot(mod_match, var.name = "distance", which = "both", type = "histogram", mirror = TRUE)
