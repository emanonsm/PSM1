love.plot(mod_match, binary = "std")
library(ggplot2)
 library(dplyr)
 library(psych)
library(MatchIt)
library(vcd)
library("cobalt")
library(DMwR2)
um <- read.csv(file = "PSM12.csv", header = TRUE, sep = ";")
colmissing <- apply(um, 2, function(x){sum(is.na(x))})
colmissing
um.c <- knnImputation(um, k = 10, meth = 'weighAvg')
colmissing <- apply(um.c, 2, function(x){sum(is.na(x))})
colmissing
m_ps <- glm(Plasma ~ x.sex + x.age + x.CT + x.CRP + x.SpO2, family = binomial("logit"), data = um.c)
mod_match <- matchit(Plasma ~ x.sex + x.age + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.3, data = um.c)
summary(mod_match, un =TRUE)
mod_match <- matchit(Plasma ~ x.sex + x.age + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.2, data = um.c)
summary(mod_match, un =TRUE)
mod_match <- matchit(Plasma ~ x.sex + x.age + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.4, data = um.c)
mod_match <- matchit(Plasma ~ x.sex + x.age + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.4, data = um.c)
summary(mod_match, un =TRUE)
mod_match <- matchit(Plasma ~ x.sex + x.age + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.5, data = um.c)
summary(mod_match, un =TRUE)
mod_match <- matchit(Plasma ~ x.age + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.5, data = um.c)
mod_match <- matchit(Plasma ~ x.age + x.CT + x.CRP + x.SpO2, method = 'nearest', distance = 'glm', caliper = 0.3, data = um.c)
